<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Score value</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %> <%= user.last_name %></td>
        <td><%= user.details %></td>
        <td><%= user.total_score %></td>
      </tr>
    <% end %>
  </tbody>
</table>